{
  "version": 2,
  "builds": [
    { "src": "*.php", "use": "@vercel/php" },
    { "src": "css/*.css", "use": "@vercel/static" },
    { "src": "js/*.js", "use": "@vercel/static" },
    { "src": "images/*", "use": "@vercel/static" },
    { "src": "uploaded_img/*", "use": "@vercel/static" },
    { "src": "screenshot/*", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "/login.php" }
  ]
}
